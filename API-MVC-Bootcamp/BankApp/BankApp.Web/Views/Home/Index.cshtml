@model List<ApplicationUser>

@{
    ViewData["Title"] = "Home Page";
}

<table class="mt-4 table table-hover table-bordered table-sm">
   <thead>
        <tr>
            <th>Name</th>
            <th>Surname</th>
            <th></th>
        </tr>
   </thead> 
   <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Surname</td>
                <td></td>
            </tr>
        }
   </tbody>
</table>


